{
  "name": "babel-es6-boilerplate",
  "version": "1.0.0",
  "description": "babel-es6-bolierplate",
  "main": "app.js",
  "scripts": {
    "build": "rimraf build && babel src --out-dir build",
    "start": "node ./scripts/start.js",
    "dev": "node_modules/.bin/nodemon --exec babel-node scripts/dev.js --watch src",
    "clean": "node ./scripts/clean.js",
    "test": "jest --watch --verbose"
  },
  "author": "",
  "license": "ISC",
  "babel": {
    "plugins": [
      "transform-async-to-generator",
      "transform-regenerator",
      "transform-class-properties",
      "transform-object-rest-spread",
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "@selenium": "./src/selenium",
            "@config": "./src/util/config",
            "@logger": "./src/util/logger",
            "@cron": "./src/util/cron",
            "@store": "./src/util/store"
          }
        }
      ]
    ],
    "presets": [
      "env"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "colors": "^1.3.1",
    "cookie-parser": "^1.4.3",
    "cron": "^1.3.0",
    "express": "^4.16.3",
    "fkill": "^5.3.0",
    "googleapis": "^32.0.0",
    "html-minifier": "^3.5.19",
    "js-yaml": "^3.11.0",
    "jsdom": "^11.6.2",
    "moment": "^2.21.0",
    "node-telegram-bot-api": "^0.30.0",
    "ps-list": "^4.0.0",
    "rimraf": "^2.6.2",
    "webdriverio": "^4.12.0",
    "winston": "^2.4.1",
    "winston-daily-rotate-file": "^1.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.4",
    "jest": "^23.5.0",
    "nodemon": "^1.17.2"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.js"
    ],
    "testEnvironment": "node"
  }
}
