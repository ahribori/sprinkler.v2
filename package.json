{
	"name": "babel-es6-boilerplate",
	"version": "1.0.0",
	"description": "babel-es6-bolierplate",
	"main": "app.js",
	"scripts": {
		"postinstall": "babel-node ./scripts/installSeleniumStandalone.js",
		"build": "node_modules/.bin/cross-env NODE_ENV=production babel src --out-dir build",
		"start": "node ./scripts/start.js",
		"dev": "node_modules/.bin/nodemon --exec babel-node scripts/dev.js --watch src"
	},
	"author": "",
	"license": "ISC",
	"babel": {
		"plugins": [
			"transform-async-to-generator",
			"transform-regenerator",
			"transform-class-properties",
			[
				"module-resolver",
				{
					"root": [
						"./src"
					],
					"alias": {
						"@selenium": "./src/selenium",
						"@tasks": "./src/tasks",
						"@logger": "./src/util/logger",
						"@cron": "./src/util/cron",
						"@schedules": "./src/schedules"
					}
				}
			]
		],
		"presets": [
			"env"
		]
	},
	"dependencies": {
		"babel-polyfill": "^6.26.0",
		"cron": "^1.3.0",
		"jsdom": "^11.5.1",
		"selenium-standalone": "^6.12.0",
		"webdriverio": "^4.10.1",
		"winston": "^2.4.0",
		"winston-daily-rotate-file": "^1.7.2"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-plugin-module-resolver": "^3.0.0",
		"babel-plugin-transform-async-to-generator": "^6.24.1",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-regenerator": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"cross-env": "^5.1.3",
		"nodemon": "^1.14.11"
	}
}
